{
    "manifest_version":2,
    "name":"Wikit",
    "version":"1.0",
    "description":"Search Wiki",
    "icons":{"128":"icon.png"},
    "page_action":{
        "default_icon":"icon.png",
        "default_popup":"popup.html",
        "default_title":"PageFontStyle"
    },
    "background":{
        "scripts":["eventPage.js"],
        "persistent":false
    },
    "permissions":[
        "tabs",
        "contextMenus"
    ]
    
}